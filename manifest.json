

{
    "manifest_version": 2,
    "name": "PrioriList",
    "version": "1.0",
    "description": "A simple todo list Chrome extension",
    "permissions": [
      "storage"
    ],
    "browser_action": {
      "default_icon": {
        "128": "logo.png"
      },
      "default_popup": "popup.html",
      "default_title": "Open Todo List"
    },
    "icons": {
      "128": "logo.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  